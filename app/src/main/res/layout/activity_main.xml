<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView3"
        android:layout_centerHorizontal="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="23dp"
        android:text="Team 14"/>

    <Button
        android:id="@+id/button"
        android:layout_centerHorizontal="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Broadcast Activity"
        android:layout_below="@+id/textView3"
        android:onClick="broadcastListener"/>

    <Button
        android:id="@+id/button2"
        android:layout_centerHorizontal="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Service Activity"
        android:layout_below="@+id/button"
        android:onClick="toService"
        app:layout_constraintEnd_toEndOf="parent"/>

    <Button
        android:id="@+id/button3"
        android:layout_centerHorizontal="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Content Provider Activity"
        android:layout_below="@+id/button2"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:id="@+id/editTextTextPersonName7"
        android:layout_centerHorizontal="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="Aditya Borikar : aborika1@asu.edu"
        android:layout_below="@+id/button3" />

    <TextView
        android:id="@+id/editTextTextPersonName8"
        android:layout_width="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:layout_below="@+id/editTextTextPersonName7"
        android:text="Pratyush Pandey : ppande28@asu.edu" />

    <TextView
        android:id="@+id/editTextTextPersonName9"
        android:layout_centerHorizontal="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:layout_below="@+id/editTextTextPersonName8"
        android:text="Sujatha Pushpraj : spushpar@asu.edu" />

    <TextView
        android:id="@+id/textView5"
        android:layout_centerHorizontal="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_below="@+id/editTextTextPersonName9" />

    <Switch
        android:id="@+id/switch1"
        android:layout_centerHorizontal="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Use Service Activity"
        android:paddingHorizontal="50dp"
        android:layout_below="@+id/textView5"/>

    <Button
        android:id="@+id/contactsButton"
        android:layout_centerHorizontal="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="gotoContacts"
        android:layout_below="@+id/switch1"
        android:text="Contacts"/>

</RelativeLayout>